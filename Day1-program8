#include<iostream>
using namespace std;
int main()
{
	int a,b,c;
	cout<<"enter the first number\n";
	cin>>a;
	cout<<"enter the second number\n";
	cin>>b;
	cout<<"enter the third number\n";
	cin>>c;
	if(a>b&&a>c)
	cout<<a<<"is greater";
	else if(b>a&&b>c)
	cout<<b<<"is greater";
	else
	cout<<c<<"is greater";
	return 0;
}
